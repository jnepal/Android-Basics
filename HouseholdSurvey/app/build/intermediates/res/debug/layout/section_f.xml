<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/brown"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    >
    <TextView
        style="@style/TitleText"
        android:text="@string/section_f"
        android:textAllCaps="true"
        />
    <!--Q79-->
    <TextView
        style="@style/NormalText"
        android:text="@string/Q_79"
        />
    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <RadioButton
            android:id="@+id/fcAttendEduMeetingYesRb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/yes"
            />
        <RadioButton
            android:id="@+id/fcAttendEduMeetingNoRb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/no"
            />
    </RadioGroup>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <TextView
            style="@style/NormalText"
            android:text="@string/yes_how_many_times"/>
        <EditText
            android:id="@+id/fcAttendHowManyTimesEt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="40dp"
            android:inputType="number"
            />
    </LinearLayout>


    <!--Q80-->
    <TextView
        style="@style/NormalText"
        android:text="@string/Q_80"
        />
    <Spinner
        android:id="@+id/fcMostLikelyToAskS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:entries="@array/when_problem_to_ask"
        ></Spinner>
    <EditText
        android:id="@+id/fcOtherToAskEt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="280dp"
        android:hint="@string/specify_if_others"
        />

    <!--Q81-->
    <TextView
        style="@style/NormalText"
        android:text="@string/Q_81"
        />
    <Spinner
        android:id="@+id/fcDecisionTakenByS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:entries="@array/decision_taken_by"
        />

    <!--Q82-->
    <TextView
        style="@style/NormalText"
        android:text="@string/Q_82"
        />
    <EditText
        android:id="@+id/fcAnySuggestionCommentEt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="40dp"
        />
</LinearLayout><!-- From: file:/C:/Users/Sabin/AndroidStudioProjects/HouseholdSurvey/app/src/main/res/layout/section_f.xml -->